<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--Mapper名称 对应infra中mapper类名-->
<mapper namespace="com.deep.infra.persistence.sql.mapper.ImmunePlanMapper">
    <insert id="setImmunePlanModel">
        INSERT INTO immune_plan(
        factory_num,
        crowd_num,
        immune_eartag,
        immune_time,
        immune_kind,
        immune_way,
        immune_quality,
        immune_during,
        operator,
        remark,
        is_pass1,
        is_pass2,
        gmt_create
        )
        VALUES(
        #{factoryNum},
        #{crowdNum},
        #{immuneEartag},
        #{immuneTime},
        #{immuneKind},
        #{immuneWay},
        #{immuneQuality},
        #{immuneDuring},
        #{operator},
        #{remark},
        #{isPass1},
        #{isPass2},
        #{gmtCreate}
        )
    </insert>
    <resultMap id="ImmunePlan" type="com.deep.domain.model.ImmunePlanModel">
        <result property="id" column="id"/>
        <result property="factoryNum" column="factory_num"/>
        <result property="crowdNum" column="crowd_num"/>
        <result property="immuneEartag" column="immune_eartag"/>
        <result property="immuneTime" column="immune_time"/>
        <result property="immuneKind" column="immune_kind"/>
        <result property="immuneWay" column="immune_way"/>
        <result property="immuneQuality" column="immune_quality"/>
        <result property="immuneDuring" column="immune_during"/>
        <result property="operator" column="operator"/>
        <result property="professor" column="professor"/>
        <result property="supervisor" column="supervisor"/>
        <result property="remark" column="remark"/>
        <result property="isPass" column="is_pass"/>
        <result property="unpassReason" column="unpass_reason"/>
        <result property="gmtCreate" column="gmt_create"/>
        <result property="gmtModified" column="gmt_modified"/>
        <result property="gmtProfessor" column="gmt_professor"/>
        <result property="gmtSupervise" column="gmt_supervise"/>
    </resultMap>


    <select id="getImmunePlanModel" resultMap="ImmunePlan">
        SELECT
        *
        FROM
        immune_plan
        WHERE
        1=1
        <if test="immunePlanModel.factoryNum != null and immunePlanModel.factoryNum != '' ">
            AND factory_num = #{immunePlanModel.factoryNum}
        </if>
        <if test="immunePlanModel.crowdNum != null and immunePlanModel.crowdNum != '' ">
            AND crowd_num = #{immunePlanModel.crowdNum}
        </if>
        <if test="immunePlanModel.immuneTimeStart != null and immunePlanModel.immuneTimeStart != '' and
                  immunePlanModel.immuneTimeEnd != null and immunePlanModel.immuneTimeEnd != '' ">
            AND immune_time BETWEEN #{immunePlanModel.immuneTimeStart} AND #{immunePlanModel.immuneTimeEnd}
        </if>
        <if test="immunePlanModel.immuneKind != null and immunePlanModel.immuneKind != '' ">
            AND immune_kind = #{immunePlanModel.immuneKind}
        </if>
        <if test="immunePlanModel.immuneWay != null and immunePlanModel.immuneWay != '' ">
            AND immune_way = #{immunePlanModel.immuneWay}
        </if>
        <if test="immunePlanModel.immuneQuality != null and immunePlanModel.immuneQuality != '' ">
            AND immune_quality = #{immunePlanModel.immuneQuality}
        </if>
        <if test="immunePlanModel.immuneDuring != null and immunePlanModel.immuneDuring != '' ">
            AND immune_during = #{immunePlanModel.immuneDuring}
        </if>
        <if test="immunePlanModel.operator != null and immunePlanModel.operator != '' ">
            AND operator = #{immunePlanModel.operator}
        </if>
        <if test="immunePlanModel.professor != null and immunePlanModel.professor != '' ">
            AND professor = #{immunePlanModel.professor}
        </if>
        <if test="immunePlanModel.supervisor != null and immunePlanModel.supervisor != '' ">
            AND supervisor = #{immunePlanModel.supervisor}
        </if>
        <if test="immunePlanModel.remark != null and immunePlanModel.remark != '' ">
            AND remark = #{immunePlanModel.remark}
        </if>
        <if test="immunePlanModel.isPass1 != null and immunePlanModel.isPass1 != '' ">
            AND is_pass1 = #{immunePlanModel.isPass1}
        </if>
        <if test="immunePlanModel.unpassReason1 != null and immunePlanModel.unpassReason1 != '' ">
            AND unpass_reason1 = #{immunePlanModel.unpassReason1}
        </if>
        <if test="immunePlanModel.isPass2 != null and immunePlanModel.isPass2 != '' ">
            AND is_pass2 = #{immunePlanModel.isPass2}
        </if>
        <if test="immunePlanModel.unpassReason2 != null and immunePlanModel.unpassReason2 != '' ">
            AND unpass_reason2 = #{immunePlanModel.unpassReason2}
        </if>
    </select>

    <select id="getImmunePlanModelByid" resultType="com.deep.domain.model.ImmunePlanModel">
        SELECT
        *
        FROM
        immune_plan
        WHERE
        id = #{id}
    </select>

    <select id="getImmunePlanModelByfactoryNumAndimmuneTime" resultMap="ImmunePlan">
        SELECT
        *
        FROM
        immune_plan
        WHERE
        factory_num = #{facctoryNum} AND immune_time BETWEEN #{gmtCreate} AND #{gmtTrans}
    </select>



    <select id="getImmunePlanModelByfactoryNumAndcrowdNumAndimmuneTime" resultMap="ImmunePlan">
        SELECT
        *
        FROM
        immune_plan
        WHERE
        factory_num = #{factoryNum} AND crowd_num = #{crowdNum} AND immune_time = #{immuneTime}
    </select>

    <select id="getImmunePlanModelByProfessor" resultMap="ImmunePlan">
        SELECT
        *
        FROM
        immune_plan
        WHERE
        is_pass1 = #{isPass1}
    </select>

    <select id="getImmunePlanModelBySupervisor" resultMap="ImmunePlan">
        SELECT
        *
        FROM
        immune_plan
        WHERE
        is_pass2 = #{isPass2}
    </select>


    <delete id="deleteImmunePlanModelByid" parameterType="BigInteger">
        DELETE FROM
        immune_plan
        WHERE
        id = #{id}
    </delete>


    <update id="updateImmunePlanModelByProfessor" parameterType="com.deep.domain.model.ImmunePlanModel">
        UPDATE immune_plan
        SET  professor = #{immunePlanModel.professor},
        is_pass1 = #{immunePlanModel.isPass1},
        unpass_reason1 = #{immunePlanModel.unpassReason1},
        gmt_professor = #{immunePlanModel.gmtProfessor}
        WHERE id = #{immunePlanModel.id}
    </update>

    <update id="updateImmunePlanModelBySupervisor" parameterType="com.deep.domain.model.ImmunePlanModel">
        UPDATE immune_plan
        SET  supervisor = #{immunePlanModel.supervisor},
        is_pass2 = #{immunePlanModel.isPass2},
        unpass_reason2 = #{immunePlanModel.unpassReason2},
        gmt_supervise = #{immunePlanModel.gmtSupervise}
        WHERE id = #{immunePlanModel.id}
    </update>
</mapper>