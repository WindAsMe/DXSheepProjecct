<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--Mapper名称 对应infra中mapper类名-->
<mapper namespace="com.deep.infra.persistence.sql.mapper.RepellentPlanMapper">
    <insert id="setRepellentPlanModel" parameterType="com.deep.domain.model.RepellentPlanModel">
        INSERT INTO repellent_plan(
        factory_num,
        crowd_num,
        repellent_eartag,
        repellent_time,
        repellent_name,
        repellent_way,
        repellent_quality,
        operator,
        remark,
        is_pass1,
        is_pass2,
        gmt_create
        )
        VALUES(
        #{factoryNum},
        #{crowdNum},
        #{repellentEartag},
        #{repellentTime},
        #{repellentName},
        #{repellentWay},
        #{repellentQuality},
        #{operator},
        #{remark},
        #{isPass1},
        #{isPass2},
        #{gmtCreate}
        )
    </insert>
    <resultMap id="RepellentPlan" type="com.deep.domain.model.RepellentPlanModel">
        <id property="factoryNum" column="factory_num"/>
        <result property="crowdNum" column="crowd_num"/>
        <result property="repellentEartag" column="repellent_eartag"/>
        <result property="repellentTime" column="repellent_time"/>
        <result property="repellentName" column="repellent_name"/>
        <result property="repellentWay" column="repellent_way"/>
        <result property="repellentQuality" column="repellent_quality"/>
        <result property="operator" column="operator"/>
        <result property="professor" column="professor"/>
        <result property="supervisor" column="supervisor"/>
        <result property="remark" column="remark"/>
        <result property="isPass" column="is_pass"/>
        <result property="unpassReason" column="unpass_reason"/>
        <result property="gmtCreate" column="gmt_create"/>
        <result property="gmtModified" column="gmt_modified"/>
        <result property="gmtProfessor" column="gmt_professor"/>
        <result property="gmtSupervise" column="gmt_supervise"/>
    </resultMap>
    <select id="getRepellentPlanModel" resultMap="RepellentPlan">
        SELECT
        *
        FROM
        repellent_plan
        WHERE
        1=1
        <if test="repellentPlanModel.factoryNum != null and repellentPlanModel.factoryNum != '' ">
            AND factory_num = #{repellentPlanModel.factoryNum}
        </if>
        <if test="repellentPlanModel.crowdNum != null and repellentPlanModel.crowdNum != '' ">
            AND crowd_num = #{repellentPlanModel.crowdNum}
        </if>
        <if test="repellentPlanModel.repellentTimeStart != null and repellentPlanModel.repellentTimeStart != '' and
                  repellentPlanModel.repellentTimeEnd != null and repellentPlanModel.repellentTimeEnd != '' ">
            AND repellent_time BETWEEN #{repellentPlanModel.repellentTimeStart} AND #{repellentPlanModel.repellentTimeEnd}
        </if>
        <if test="repellentPlanModel.repellentName != null and repellentPlanModel.repellentName != '' ">
            AND repellent_name = #{repellentPlanModel.repellentName}
        </if>

        <if test="repellentPlanModel.repellentWay != null and repellentPlanModel.repellentWay != '' ">
            AND repellent_way = #{repellentPlanModel.repellentWay}
        </if>
        <if test="repellentPlanModel.repellentQuality != null and repellentPlanModel.repellentQuality != '' ">
            AND repellent_quality = #{repellentPlanModel.repellentQuality}
        </if>

        <if test="repellentPlanModel.operator != null and repellentPlanModel.operator != '' ">
            AND operator = #{repellentPlanModel.operator}
        </if>
        <if test="repellentPlanModel.professor != null and repellentPlanModel.professor != '' ">
            AND professor = #{repellentPlanModel.professor}
        </if>
        <if test="repellentPlanModel.supervisor != null and repellentPlanModel.supervisor != '' ">
            AND supervisor = #{repellentPlanModel.supervisor}
        </if>
        <if test="repellentPlanModel.remark != null and repellentPlanModel.remark != '' ">
            AND remark = #{repellentPlanModel.remark}
        </if>
        <if test="repellentPlanModel.isPass1 != null and repellentPlanModel.isPass1 != '' ">
            AND is_pass1 = #{repellentPlanModel.isPass1}
        </if>
        <if test="repellentPlanModel.unpassReason1 != null and repellentPlanModel.unpassReason1 != '' ">
            AND unpass_reason1 = #{repellentPlanModel.unpassReason1}
        </if>
        <if test="repellentPlanModel.isPass2 != null and repellentPlanModel.isPass2 != '' ">
            AND is_pass2 = #{repellentPlanModel.isPass2}
        </if>
        <if test="repellentPlanModel.unpassReason2 != null and repellentPlanModel.unpassReason2 != '' ">
            AND unpass_reason2 = #{repellentPlanModel.unpassReason2}
        </if>
    </select>

    <select id="getRepellentPlanModelByfactoryNumAndrepellentTime" resultMap="RepellentPlan">
        SELECT
        *
        FROM
        repellent_plan
        WHERE
        factory_num = #{factoryNum} AND repellent_time BETWEEN #{gmtCreate} AND #{gmtTrans}
    </select>

    <select id="getRepellentPlanModelByfactoryNumAndrepellentTimeAndrepellentName" resultMap="RepellentPlan">
        SELECT
        *
        FROM
        repellent_plan
        WHERE
        factory_num = #{factoryNum} AND repellent_time = #{repellentTime} AND repellent_name = #{repellentName}
    </select>

    <select id="getRepellentPlanModelByid" resultMap="RepellentPlan">
        SELECT
        *
        FROM
        repellent_plan
        WHERE
        id = #{id}
    </select>

    <select id="getRepellentPlanModelByProfessor" resultMap="RepellentPlan">
        SELECT
        *
        FROM
        repellent_plan
        WHERE
        is_pass1 = #{isPass1}
    </select>

    <select id="getRepellentPlanModelBySupervisor" resultMap="RepellentPlan">
        SELECT
        *
        FROM
        repellent_plan
        WHERE
        is_pass2 = #{isPass2}
    </select>

    <delete id="deleteRepellentPlanModelByid" parameterType="BigInteger">
        DELETE FROM
        repellent_plan
        WHERE
        id = #{id}
    </delete>


    <update id="updateRepellentPlanModelByProfessor" parameterType="com.deep.domain.model.RepellentPlanModel">
        UPDATE repellent_plan
        SET  professor = #{repellentPlanModel.professor},
        is_pass1 = #{repellentPlanModel.isPass1},
        unpass_reason1 = #{repellentPlanModel.unpassReason1},
        gmt_professor = #{repellentPlanModel.gmtProfessor}
        WHERE id = #{repellentPlanModel.id}
    </update>

    <update id="updateRepellentPlanModelBySupervisor" parameterType="com.deep.domain.model.RepellentPlanModel">
        UPDATE repellent_plan
        SET  supervisor = #{repellentPlanModel.supervisor},
        is_pass2 = #{repellentPlanModel.isPass2},
        unpass_reason2 = #{repellentPlanModel.unpassReason2},
        gmt_supervise = #{repellentPlanModel.gmtSupervise}
        WHERE id = #{repellentPlanModel.id}
    </update>
</mapper>